{
  "name": "visualnotions",
  "version": "1.0.0",
  "description": "visualnotions",
  "main": "index.js",
  "scripts": {
    "start": "cd src && nodemon --ext js,json,hbs,css,scss -x \"npm run build-css && node index.js\" --ignore \"src/views/**/*.css\"",
    "build-css": "npx node-sass src/views --output src/views"
  },
