{
  "name": "visualnotions",
  "version": "1.0.0",
  "description": "visualnotions",
  "main": "index.js",
  "scripts": {
    "watch": "concurrently \"npm:watch:server\" \"npm:watch:scss\"",
    "watch:server": "nodemon --ext js,json,hbs -x \"npm start\"",
    "watch:scss": "node-sass -w src/views --output src/views",
    "start": "cd src && node index.js"
  },
  "author": "Vitali Zaidman (vzaidman@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6"
  },
  "devDependencies": {
    "node-sass": "^7.0.3",
    "nodemon": "^2.0.20"
  }
}
